{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "swaply",
  "version": "0.1.0",
  "identifier": "com.swaply.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "npm run generate",
    "frontendDist": "../.output/public"
  },
  "app": {
    "windows": [
      {
        "title": "swaply",
        "width": 800,
        "height": 600
      }
    ],
    "security": { "devCsp": "", "csp": null }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
