{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "log:default",
    "admob:allow-adCreate",
    "admob:allow-adLoad",
    "admob:allow-adShow",
    {
      "identifier": "http:default",
      "allow": [{ "url": "http://192.168.1.100:*" }]
    }
  ]
}
